{
  "trailingSlash": "always",
  "globalHeaders": {
    "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'"
  },
  "routes": [
    {
      "route": "/login",
      "redirect": "/.auth/login/github"
    },
    {
      "route": "/mypage",
      "allowedRoles": [
        "authenticated"
      ]
    },
    {
      "route": "/logout",
      "redirect": "/.auth/logout"
    }
  ]
}
